<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.streetshoot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.STREETSHOOT">

        <!-- ACTIVIDAD PRINCIPAL -->
        <activity
            android:name=".InicioSesion"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HubActivity" />
        <activity android:name=".ZonasActivity" />
        <activity android:name=".StreetspotsActivity" />
        <activity android:name=".RetosActivity" />
        <activity android:name=".NotificacionesActivity" />
        <activity android:name=".PersonalizarPerfilActivity" />





        <!-- REGISTRO -->
        <activity
            android:name=".Registro"
            android:exported="false" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>
